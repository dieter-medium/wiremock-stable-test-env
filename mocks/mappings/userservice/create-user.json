{
  "request": {
    "method": "POST",
    "urlPattern": "^/users$",
    "headers": {
      "Host": {
        "matches": "userservice.localtest.me.*"
      },
      "Content-Type": {
        "equalTo": "application/json"
      }
    },
    "bodyPatterns": [
      {
        "matchesJsonPath": "$.firstName"
      },
      {
        "matchesJsonPath": "$.lastName"
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "bodyFileName": "userservice/create-user.json"
  }
}